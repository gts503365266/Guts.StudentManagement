@model IEnumerable<StudentManagement.Models.Student>
@{
    ViewBag.Title = "学生列表页面";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>名称</th>
            <th>邮箱</th>
            <th>班级名称</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.ClassName</td>
            </tr>
        }
    </tbody>
</table>
